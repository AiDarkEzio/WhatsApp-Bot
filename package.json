{
  "name": "whatsapp-bot",
  "version": "1.0.0",
  "description": "This TypeScript WhatsApp bot is a simple chatbot created using the `@adiwajshing/baileys` library.A TypeScript WhatsApp bot is a chatbot programmed in TypeScript that interacts with users on WhatsApp. It responds to commands, provides information, and performs tasks",
  "main": "index.js",
  "module": "ES2022",
  "scripts": {
    "build": "tsc",
    "build:js": "npm i && tsc",
    "auth:recreate": "node index.js",
    "auth:remove": "rm -rf database/baileys/store_multi.json",
    "start": "supervisor index.js",
    "test:clear:build": "npm run build && clear && npm run test",
    "tests:clear:build": "npm run build && clear && npm run tests",
    "tests:build": "tsc tests/index.ts && node tests/index.js",
    "tests": "node tests/index.js",
    "test": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AiDarkEzio/WhatsApp-Bot.git"
  },
  "author": "AiDarkEzio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AiDarkEzio/WhatsApp-Bot/issues"
  },
  "homepage": "https://github.com/AiDarkEzio/WhatsApp-Bot#readme",
  "dependencies": {
    "@adiwajshing/baileys": "^5.0.0",
    "@aidarkezio/main-func": "^2.3.0",
    "@hapi/boom": "^10.0.1",
    "@types/axios": "^0.14.0",
    "@types/ffmpeg": "^1.0.4",
    "@types/node-fetch": "^2.6.3",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "jimp": "^0.16.13",
    "json2csv": "^6.0.0-alpha.2",
    "link-preview-js": "^3.0.4",
    "node-fetch": "^3.3.1",
    "pino": "^8.11.0",
    "qrcode-terminal": "^0.12.0",
    "supervisor": "^0.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "yt-search": "^2.10.4",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node": "^18.15.11",
    "@types/ws": "^8.5.4",
    "@types/yt-search": "^2.3.2"
  }
}
